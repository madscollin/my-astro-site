---
import Layout from "../layouts/Layout.astro";
import EmailPreview from "../components/EmailPreview.astro";
---

<Layout title="Signature generator">
    <main
        x-data="{
            signature: {
                companies: [
                    { id: 1, name: 'Speedline' },
                    { id: 2, name: 'Cykelshoppen' },
                    { id: 3, name: 'Both' }
                ],
                name: '',
                position: '',
                email: '',
                phone: '',
                selectedCompany: '1',
                socialMedia: false,
                
              }
        }"
        class="container mx-auto px-5"
    >
        <h1 class="text-center mt-48 text-5xl font-extrabold">
            Email Signature Generator
        </h1>
        <div class="w-full space-x-12 flex mt-10">
            <div class="w-4/12">
                <p class="text-xl font-extralight text-gray-600">
                    Enter your details
                </p>
                <div class="shadow-lg bg-white p-8 rounded-xl mt-4">
                    <div class="mt-4">
                        <label class="font-bold">Company</label>
                        <ul class="flex items-center mt-1">
                            <template
                                x-for="company in signature.companies"
                                :key="company.id"
                            >
                                <li
                                    class="flex items-center space-x-1 font-light mr-3"
                                >
                                    <input
                                        x-model="signature.selectedCompany"
                                        type="radio"
                                        :value="company.id"
                                        :id="company.id"
                                        class="form-radio"
                                    />
                                    <label
                                        :for="company.id"
                                        x-text="company.name"></label>
                                </li>
                            </template>
                        </ul>
                    </div>

                    <div class="mt-4">
                        <label for="signatureName" class="font-bold">Name</label
                        >
                        <input
                            placeholder="First and last name"
                            id="signatureName"
                            class="form-input mt-1"
                            type="text"
                            x-model="signature.name"
                        />
                    </div>
                    <div class="mt-4">
                        <label for="signaturePosition" class="font-bold"
                            >Position</label
                        >
                        <input
                            placeholder="What you do"
                            id="signaturePosition"
                            class="form-input mt-1"
                            type="text"
                            x-model="signature.position"
                        />
                    </div>
                    <div class="mt-4">
                        <label for="signatureEmail" class="font-bold"
                            >Email</label
                        >
                        <input
                            placeholder="Your email"
                            id="signatureEmail"
                            class="form-input mt-1"
                            type="email"
                            x-model="signature.email"
                        />
                    </div>
                    <div class="mt-4">
                        <label for="signaturePhone" class="font-bold"
                            >Phone</label
                        >
                        <input
                            placeholder="23 23 23 23"
                            id="signaturePhone"
                            class="form-input mt-1"
                            type="tel"
                            x-model="signature.phone"
                        />
                    </div>

                    <div class="mt-4">
                        <label for="signatureSocial" class="font-bold">Social Media?</label
                        >
                        <div class="flex items-center space-x-2">
                        <input
                            x-model="signature.socialMedia"
                            type="checkbox"
                            :value="signature.socialMedia"
                            class="form-checkbox"
                        />
                        <template x-if="signature.socialMedia">
                            <span class="font-light">Yes</span>
                        </template>
                        <template x-if="!signature.socialMedia">
                            <span class="font-light">No</span>
                        </template>
                       
                        </div>
                    </div>

                    <button
                        class="w-full p-3 bg-orange-600 text-orange-50 rounded-md text-xl mt-6 js-copy-btn"
                    >
                        Copy signature to clipboard
                    </button>
                </div>
            </div>
            <div class="w-8/12">
                <p class="text-xl font-extralight text-gray-600">
                    Signature preview
                </p>
                <div
                    class="border rounded-lg border-gray-300 p-8 mt-4 bg-white shadow-xl"
